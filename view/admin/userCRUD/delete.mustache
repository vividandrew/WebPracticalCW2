{{>admin.head.shared}}
<h1>{{user.fullname}}</h1>
<h5>Username:{{user.username}}</h5>
<h5>Role: {{user.role}}</h5>
<h3>Are you certain you want to delete this user?</h3>
<form action="/admin/user/delete/{{user._id}}" method="POST"><button role="menuitem" tabindex="-1" type="submit" class="block px-4 py-2 text-sm text-gray-700">Delete</button></form>
{{>admin.tail.shared}}